{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions":[
        {
            "name": "frontend",
            "image": "iaminteger87/docker-frontend",
            "hostname" :"frontend",
            "essential": false,
            "memory": 128
        },
        {
            "name": "backend",
            "image": "iaminteger87/docker-backend",
            "hostname" :"backend",
            "essential": false,
            "memory": 128
        },
        {
            "name": "nginx",
            "image": "iaminteger87/docker-nginx",
            "hostname" :"nginx",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 80,
                    "containerPort": 80
                }
            ],
            "links": ["frontend", "backend"],
            "memory": 128
        }

    ]
}